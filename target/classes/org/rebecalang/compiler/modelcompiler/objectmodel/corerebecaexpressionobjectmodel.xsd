<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified"
	targetNamespace="http://rebecalang.org/compiler/modelcompiler/corerebecaexpression"
	xmlns="http://rebecalang.org/compiler/modelcompiler/corerebecaexpression"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="1.0">

	<xs:complexType name="Annotation">
		<xs:sequence>
			<xs:element name="value" type="Expression" />
		</xs:sequence>
		<xs:attribute name="identifier" type="xs:string" />
		<xs:attribute name="lineNumber" type="xs:int" />
		<xs:attribute name="character" type="xs:int" />
	</xs:complexType>

	<xs:complexType name="Statement">
		<xs:sequence>
			<xs:element name="annotations" type="Annotation"
				maxOccurs="unbounded" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="lineNumber" type="xs:int" />
		<xs:attribute name="character" type="xs:int" />
	</xs:complexType>

	<xs:complexType name="Expression">
		<xs:complexContent>
			<xs:extension base="Statement">
				<xs:sequence>
					<xs:element name="type" type="Type" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TernaryExpression">
		<xs:complexContent>
			<xs:extension base="Expression">
				<xs:sequence>
					<xs:element name="condition" type="Expression" />
					<xs:element name="left" type="Expression" />
					<xs:element name="right" type="Expression" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="InstanceofExpression">
		<xs:complexContent>
			<xs:extension base="Expression">
				<xs:sequence>
					<xs:element name="value" type="Expression" />
					<xs:element name="evaluationType" type="Type" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="BinaryExpression">
		<xs:complexContent>
			<xs:extension base="Expression">
				<xs:sequence>
					<xs:element name="left" type="Expression" />
					<xs:element name="right" type="Expression" />
				</xs:sequence>
				<xs:attribute name="operator" type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="UnaryExpression">
		<xs:complexContent>
			<xs:extension base="Expression">
				<xs:sequence>
					<xs:element name="expression" type="Expression" />
				</xs:sequence>
				<xs:attribute name="operator" type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CastExpression">
		<xs:complexContent>
			<xs:extension base="Expression">
				<xs:sequence>
					<xs:element name="expression" type="Expression" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="NonDetExpression">
		<xs:complexContent>
			<xs:extension base="Expression">
				<xs:sequence>
					<xs:element name="choices" type="Expression"
						maxOccurs="unbounded" minOccurs="1" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Literal">
		<xs:complexContent>
			<xs:extension base="Expression">
				<xs:attribute name="literalValue" type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="PlusSubExpression">
		<xs:complexContent>
			<xs:extension base="Expression">
				<xs:sequence>
					<xs:element name="value" type="Expression" />
				</xs:sequence>
				<xs:attribute name="operator" type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="PrimaryExpression">
		<xs:complexContent>
			<xs:extension base="Expression">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Label">
		<xs:attribute name="name" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="RebecInstantiationPrimary">
		<xs:complexContent>
			<xs:extension base="PrimaryExpression">
				<xs:sequence>
					<xs:element name="bindings" type="Expression"
						maxOccurs="unbounded" minOccurs="0" />
					<xs:element name="arguments" type="Expression"
						maxOccurs="unbounded" minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="DotPrimary">
		<xs:complexContent>
			<xs:extension base="PrimaryExpression">
				<xs:sequence>
					<xs:element name="left" type="Expression" />
					<xs:element name="right" type="PrimaryExpression" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TermPrimary">
		<xs:complexContent>
			<xs:extension base="PrimaryExpression">
				<xs:sequence>
					<xs:element name="label" type="Label" />
					<xs:element name="parentSuffixPrimary"
						type="ParentSuffixPrimary" />
					<xs:element name="indices" type="Expression"
						maxOccurs="unbounded" minOccurs="1" />
				</xs:sequence>
				<xs:attribute name="name" type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ParentSuffixPrimary">
		<xs:sequence>
			<xs:element name="arguments" type="Expression"
				maxOccurs="unbounded" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="lineNumber" type="xs:int" />
		<xs:attribute name="character" type="xs:int" />
	</xs:complexType>

	<xs:complexType name="Type">
		<xs:sequence>
			<xs:element name="typeSystem" type="TypeSystem"
				maxOccurs="1" minOccurs="1" />
		</xs:sequence>
		<xs:attribute name="lineNumber" type="xs:int" />
		<xs:attribute name="character" type="xs:int" />
	</xs:complexType>

	<xs:complexType name="TypeSystem">
		<xs:annotation>
			<xs:appinfo>
				<jxb:class
					ref="org.rebecalang.compiler.modelcompiler.abstractrebeca.AbstractTypeSystem" />
			</xs:appinfo>
		</xs:annotation>
	</xs:complexType>


	<xs:complexType name="ArrayType">
		<xs:complexContent>
			<xs:extension base="Type">
				<xs:sequence>
					<xs:element name="dimensions" type="xs:int"
						maxOccurs="unbounded" minOccurs="1" />
					<xs:element name="ordinaryPrimitiveType"
						type="OrdinaryPrimitiveType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="GenericTypeInstance">
		<xs:complexContent>
			<xs:extension base="Type">
				<xs:sequence>
					<xs:element name="base" type="GenericType" maxOccurs="1"
						minOccurs="1" />
					<xs:element name="parameters" type="Type"
						maxOccurs="unbounded" minOccurs="1" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="GenericType">
		<xs:complexContent>
			<xs:extension base="Type">
				<xs:attribute name="name" type="xs:string" />
				<xs:attribute name="numberOfParameters" type="xs:int" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="OrdinaryPrimitiveType">
		<xs:complexContent>
			<xs:extension base="Type">
				<xs:attribute name="name" type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>